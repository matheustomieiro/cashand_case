[application]

config/name="New Rakugo Game"
run/main_scene="res://game/Start/Start.tscn"
config/project_settings_override="res://game/rakugo.cfg"
boot_splash/image="res://graphics/splash.png"
boot_splash/fullsize=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://graphics/window_icon.png"
config/auto_accept_quit=false
config/quit_on_go_back=false
rakugo/version="0.0.1"
rakugo/game_credits="Jeremi {i}jebedaia360{/i} Biernacki, kurinoku"
rakugo/debug=false
rakugo/save_folder="saves"
rakugo/default_kind="nvl"
rakugo/test_saves=false
rakugo/markup="renpy"
rakugo/default_mkind="grid"
rakugo/default_mcolumns=2.0
rakugo/default_manchor="center"
rakugo/punctuation_pause="1"
rakugo/default_mcolums=2.0
rakugo/theme="res://themes/question/question.tres"
rakugo/scene_links="res://game/scene_links.tres"
rakugo/scenes_links="res://game/scenes_links.tres"

[autoload]

TextPasser="*res://addons/Rakugo/lib/Text.tscn"
Rakugo="*res://addons/Rakugo/Rakugo.tscn"
settings="*res://addons/Rakugo/gui/settings.gd"
Window="*res://gui/Window/Window.tscn"

[debug]

gdscript/completion/autocomplete_setters_and_getters=true
gdscript/warnings/unassigned_variable=false
gdscript/warnings/unassigned_variable_op_assign=false
gdscript/warnings/unused_variable=false
gdscript/warnings/unused_argument=false
gdscript/warnings/unused_signal=false
gdscript/warnings/return_value_discarded=false

